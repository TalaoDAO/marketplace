<?php

/**
 * @file
 * Install and uninstall hooks for Filter Harmonizer module.
 */

/**
 * Implements hook_uninstall().
 */
function filter_harmonizer_uninstall() {
  variable_del('filter_harmonizer_contextual_filters');
  variable_del('filter_harmonizer_always');
  variable_del('filter_harmonizer_fill_exposed');
}
